<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  
  <PreceptDef ParentName="PreceptRoleSingleBase">
    <defName>VME_IdeoRole_LeaderSecond</defName>
    <label>second triumvir</label>
    <description>A leader who holds the group together and represents your faction in diplomatic interactions.</description>
    <iconPath>UI/Roles/Leader</iconPath>
	 <nameMaker>VME_NamerSecondTriumvir</nameMaker>
   
    <displayOrderInImpact>1000</displayOrderInImpact>
    <restrictToSupremeGenderChance>1</restrictToSupremeGenderChance>
    <expectationsOffset>2</expectationsOffset>
    <roleApparelRequirements>
      <li>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_Headwrap</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>Neck</li>
            <li>Shoulders</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_Broadwrap</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <allowedFactionCategoryTags><li>Tribal</li></allowedFactionCategoryTags>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_WarMask</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <disallowedFactionCategoryTags><li>Tribal</li></disallowedFactionCategoryTags>
        <anyMemeRequired><li>Transhumanist</li></anyMemeRequired>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_Slicecap</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_VisageMask</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <anyMemeRequired><li>Supremacist</li><li>Collectivist</li></anyMemeRequired>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_AuthorityCap</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <anyMemeRequired><li>Rancher</li></anyMemeRequired>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_CowboyHat</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <allowedFactionCategoryTags><li>Tribal</li></allowedFactionCategoryTags>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_TribalHeaddress</li>
          </requiredDefs>
        </requirement>
      </li>
      <li>
        <allowedFactionCategoryTags><li>Tribal</li></allowedFactionCategoryTags>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_WarMask</li>
          </requiredDefs>
        </requirement>
      </li>
      <li MayRequire="Ludeon.RimWorld.Royalty">
        <disallowedFactionCategoryTags><li>Tribal</li></disallowedFactionCategoryTags>
        <requirement>
          <bodyPartGroupsMatchAny>
            <li>UpperHead</li>
            <li>FullHead</li>
          </bodyPartGroupsMatchAny>
          <requiredDefs>
            <li>Apparel_Beret</li>
          </requiredDefs>
        </requirement>
      </li>
    </roleApparelRequirements>
    <roleApparelRequirementCountCurve>
      <points>
        <li>(0.5, 1)</li>
        <li>(1.0, 2)</li>
      </points>
    </roleApparelRequirementCountCurve>
    <grantedAbilities>
      <li>LeaderSpeech</li>
      <li>WorkDrive</li>
      <li>CombatCommand</li>
      <li>Trial</li>
    </grantedAbilities>
    <roleTags><li>Leader</li></roleTags>
	<requiredMemes>
      <li>VME_Republic</li>
      
    </requiredMemes>
  </PreceptDef>
  
