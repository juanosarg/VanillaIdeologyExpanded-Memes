<?xml version="1.0" encoding="utf-8" ?>
<Defs>



  <ThingDef ParentName="BuildingFloorCoveringMediumBase">
    <defName>VME_TaoistSlab_Medium</defName>
    <label>shintao slab (medium)</label>
    <description>A stone bas relief of shintao imagery for decorating a floor.</description>
    <dominantStyleCategory>VME_Shintaoism</dominantStyleCategory>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/BuildingStyles/Taoist/Slab/Medium</texPath>
      <drawSize>(3,3)</drawSize>
    </graphicData>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <stuffCategories>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </ThingDef>

